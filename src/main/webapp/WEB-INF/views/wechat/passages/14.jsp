                    <!--这里最多最多3张图片-->
                    <img src="${contextPath}/static/img/wechat/show/passages/14/achiev_1.jpg">
                    <img src="${contextPath}/static/img/wechat/show/passages/14/achiev_2.jpg">
                    <img src="${contextPath}/static/img/wechat/show/passages/14/achiev_3.jpg">
                </div>
            </div>
        </div>

        <div class="panel-footer"></div>
    </div>
</div>
